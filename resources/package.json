{
  "name": "resources",
  "version": "1.0.0",
  "description": "",
  "main": "getsecrets.js",
  "scripts": {
    "test": "jest",
    "build": "npm run prebuild && npm run combined-build",
    "prebuild": "rm -rf dist",
    "combined-build": "npm run build-getSHEsecret && npm run build-postSHEsecret && npm run build-getE2Esecret && npm run build-postE2Esecret",
    "build-getSHEsecret": "esbuild getSHEsecrets.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/getSHEsecret.js",
    "build-postSHEsecret": "esbuild postSHEsecrets.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/postSHEsecret.js",
    "build-getE2Esecret": "esbuild getE2Esecrets.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/getE2Esecret.js",
    "build-postE2Esecret": "esbuild postE2Esecrets.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/postE2Esecret.js",
    "postbuild": "cd dist && zip -r getSHEsecret.zip getSHEsecret.js* && zip -r postSHEsecret.zip postSHEsecret.js* && zip -r getE2Esecret.zip getE2Esecret.js* && zip -r postE2Esecret.zip postE2Esecret.js* && rm -rf *.js*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/aws-lambda": "^8.10.130",
    "esbuild": "^0.19.9",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "aws-sdk-client-mock": "^3.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
